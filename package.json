{
  "name": "vue-aplayer",
  "description": "A beautiful HTML5 music player for Vue.js",
  "version": "2.0.0-beta.1",
  "author": "u3u <qwq@qwq.cat>",
  "license": "MIT",
  "homepage": "https://github.com/MoePlayer/vue-aplayer#readme",
  "repository": {
    "url": "git+https://github.com/MoePlayer/vue-aplayer.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/MoePlayer/vue-aplayer/issues"
  },
  "keywords": [
    "player",
    "aplayer",
    "vue-aplayer",
    "music",
    "html5",
    "audio",
    "media"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --open",
    "build:example": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "lint:prettier": "prettier-eslint-check \"**/*.{js,jsx,json,ts,tsx,scss,vue,md}\"",
    "format": "prettier-eslint \"**/*.{js,jsx,json,ts,tsx,scss,vue,md}\" --write",
    "test:types": "dtslint types",
    "contributors": "all-contributors"
  },
  "dependencies": {
    "balloon-css": "^0.5.0",
    "classnames": "^2.2.6",
    "console.img": "^2.1.3",
    "hammerjs": "^2.0.8",
    "hls.js": "^0.10.1",
    "lodash.shuffle": "^4.2.0",
    "store": "^2.0.12",
    "vue": "^2.5.16",
    "vue-class-component": "^6.2.0",
    "vue-property-decorator": "^7.0.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.4",
    "@types/hammerjs": "^2.0.35",
    "@types/hls.js": "^0.10.1",
    "@types/lodash.shuffle": "^4.2.3",
    "@types/storejs": "^2.0.2",
    "@vue/cli-plugin-babel": "^3.0.0-rc.3",
    "@vue/cli-plugin-eslint": "^3.0.0-rc.3",
    "@vue/cli-plugin-typescript": "^3.0.0-rc.3",
    "@vue/cli-service": "^3.0.0-rc.3",
    "@vue/eslint-config-airbnb": "^3.0.0-rc.3",
    "@vue/eslint-config-typescript": "^3.0.0-rc.3",
    "all-contributors-cli": "^5.2.1",
    "dtslint": "^0.3.0",
    "lint-staged": "^7.2.0",
    "node-sass": "^4.9.0",
    "prettier": "^1.13.7",
    "prettier-eslint-check": "^1.0.1",
    "prettier-eslint-cli": "^4.7.1",
    "sass-loader": "^7.0.3",
    "vue-svg-loader": "^0.5.0",
    "vue-template-compiler": "^2.5.16",
    "vue-tsx-support": "^1.3.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.{js,jsx,json,ts,tsx,scss,vue,md}": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
